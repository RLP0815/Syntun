package com.syntun.entity;

/**
 * 
 */
public class ProductPlatformList {
    //主键ID
	private int id;
	private String operationProductId;
	private int uid;
	private int shopId;
    //平台ID
	private int platformId;
	private String skuId;
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getOperationProductId() {
		return operationProductId;
	}
	public void setOperationProductId(String operationProductId) {
		this.operationProductId = operationProductId;
	}
	public int getUid() {
		return uid;
	}
	public void setUid(int uid) {
		this.uid = uid;
	}
	public int getShopId() {
		return shopId;
	}
	public void setShopId(int shopId) {
		this.shopId = shopId;
	}
	public int getPlatformId() {
		return platformId;
	}
	public void setPlatformId(int platformId) {
		this.platformId = platformId;
	}
	public String getSkuId() {
		return skuId;
	}
	public void setSkuId(String skuId) {
		this.skuId = skuId;
	}
    
}